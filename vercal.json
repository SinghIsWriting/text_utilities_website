{
    "version": 2,
    "builds": [
        {
            "src": "textUtils/wsgi.py",
            "use": "@vercal/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.10"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercal/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "textUtils/wsgi.py"
        }
    ]
}